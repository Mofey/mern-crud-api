{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "https://mern-crud-frontend-orpin.vercel.app/",
          "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
          "Access-Control-Allow-Headers": "Content-Type"
        }
      }
    ]
  }